{
	"name": "pdf-generator",
	"productName": "pdf-generator",
	"version": "1.0.0",
	"description": "PDF generator applicator",
	"private": true,
	"main": ".webpack/main",
	"engines": {
		"node": ">=10.17.0 <=12.19.0"
	},
	"scripts": {
		"start": "electron-forge start",
		"build": "electron-forge make",
		"build:mac": "electron-forge make --platform darwin",
		"build:linux": "electron-forge make --platform linux --targets deb,rpm",
		"build:windows": "electron-forge make --platform=win32",
		"test": "jest",
		"test:watch": "npm run test -- --watch"
	},
	"keywords": [],
	"author": {
		"name": "Debotos",
		"email": "debotosdas@gmail.com"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"name": "PDF Generator",
				"icon": "./src/assets/logo"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "PDF Generator"
					}
				},
				{
					"name": "@electron-forge/maker-dmg",
					"platforms": [
						"darwin"
					],
					"config": {
						"background": "./src/assets/cover-mac-app-dmg-bundle.png",
						"format": "ULFO",
						"name": "PDF Generator",
						"icon": "./src/assets/logo.icns"
					}
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {
						"maintainer": "Debotos",
						"icon": "./src/assets/logo.png"
					}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {
						"maintainer": "Debotos",
						"icon": "./src/assets/logo.png"
					}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./public/index.html",
									"js": "./src/renderer/index.tsx",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"dependencies": {
		"@ant-design/icons": "^4.3.0",
		"antd": "^4.8.4",
		"electron-squirrel-startup": "^1.0.0",
		"nanoid": "^3.1.16",
		"pdfkit": "^0.11.0",
		"react": "17.0.0",
		"react-dom": "17.0.0",
		"react-router-dom": "^5.2.0",
		"styled-components": "^5.2.1"
	},
	"devDependencies": {
		"@babel/core": "7.0.0",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.10.1",
		"@babel/preset-typescript": "^7.10.1",
		"@electron-forge/cli": "^6.0.0-beta.54",
		"@electron-forge/maker-deb": "^6.0.0-beta.54",
		"@electron-forge/maker-dmg": "^6.0.0-beta.51",
		"@electron-forge/maker-rpm": "^6.0.0-beta.54",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.54",
		"@electron-forge/plugin-webpack": "6.0.0-beta.51",
		"@hot-loader/react-dom": "^17.0.0",
		"@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
		"@testing-library/jest-dom": "^5.9.0",
		"@testing-library/react": "^10.2.1",
		"@types/jest": "^25.2.3",
		"@types/react": "^16.9.56",
		"@types/react-dom": "^16.9.9",
		"@types/react-router-dom": "^5.1.6",
		"@types/styled-components": "^5.1.4",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "2.0.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"css-loader": "^3.5.3",
		"electron": "^10.1.5",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-custom-alias": "^1.2.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-module-resolver": "^1.0.0",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"file-loader": "^6.0.0",
		"fork-ts-checker-webpack-plugin": "^3.1.1",
		"husky": "^4.2.5",
		"jest": "^26.0.1",
		"less": "^3.12.2",
		"less-loader": "^7.1.0",
		"node-loader": "^0.6.0",
		"node-sass": "^4.14.1",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"react-hot-loader": "^4.12.21",
		"react-is": "^17.0.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.1",
		"ts-loader": "^7.0.5",
		"typescript": "^4.0.5",
		"url-loader": "^4.1.0",
		"webpack": "^5.4.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
